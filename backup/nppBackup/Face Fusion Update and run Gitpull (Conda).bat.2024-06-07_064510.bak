@echo off

cd C:\AI\Facefusion-QueueItUp

echo Backup 
copy /Y facefusion.ini backup\Keep_this_previous_facefusion.ini
copy /Y facefusion\uis\layouts\QueueItUp.py backup\Keep_this_QueueItUp.py
copy /Y facefusion\uis\layouts\editqueue.py backup\Keep_this_editqueue.py
copy /Y facefusion\normalizer.py backup\Keep_this_normalizer.py 
copy /Y facefusion\content_analyser.py backup\Keep_This_content_analyser.py 


echo Git pull
git pull --verbose
echo Restore
copy /Y backup\Keep_this_previous_facefusion.ini facefusion.ini
copy /Y backup\Keep_this_normalizer.py facefusion\normalizer.py
copy /Y backup\Keep_This_content_analyser.py facefusion\content_analyser.py
call conda activate facefusion

echo Running FaceFusion...
python run.py

echo FaceFusion has closed.
echo Press any key within 3 seconds to keep this terminal open...
choice /C YN /N /D N /T 3 >nul
